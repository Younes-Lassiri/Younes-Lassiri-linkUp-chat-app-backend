{
    "version": 2,
    "builds": [
        {
            "src": "/api/index.php",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}